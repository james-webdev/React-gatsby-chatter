{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop -H 0.0.0.0",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "start": "npm run develop",
    "format": "prettier --config ./.prettierrc --write src/**/*.js",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "test": "ava **/*.test.js --verbose"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "babel-plugin-styled-components": "^1.10.6",
    "gatsby": "^2.16.1",
    "gatsby-background-image": "^0.8.15",
    "gatsby-image": "^2.2.30",
    "gatsby-plugin-sharp": "^2.2.36",
    "gatsby-plugin-styled-components": "^3.1.12",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-transformer-sharp": "^2.3.2",
    "gsap": "^2.1.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-lottie": "^1.2.3",
    "react-typography": "^0.16.19",
    "styled-components": "^4.4.1",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "gatsby-plugin-typography": "^2.3.14",
    "gatsby-source-filesystem": "^2.1.33",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
